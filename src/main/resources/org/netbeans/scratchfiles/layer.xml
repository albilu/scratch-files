<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
	<folder name="Favorites">
		<file name="ScratchFiles.shadow">
			<attr name="originalFile" methodvalue="org.netbeans.scratchfiles.ScratchFilesFolder.getScratchFilesFolder"/>
			<attr name="position" intvalue="200"/>
		</file>
	</folder>
    <folder name="Services">
        <folder name="AutoupdateType">
            <file name="io_github_albilu_scratchfiles_update_center.instance">
                <attr name="displayName" bundlevalue="org.netbeans.scratchfiles.Bundle#Services/AutoupdateType/io_github_albilu_scratchfiles_update_center.instance"/>
                <attr name="enabled" boolvalue="true"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.modules.autoupdate.updateprovider.AutoupdateCatalogFactory.createUpdateProvider"/>
                <attr name="instanceOf" stringvalue="org.netbeans.spi.autoupdate.UpdateProvider"/>
                <attr name="url" bundlevalue="org.netbeans.scratchfiles.Bundle#io_github_albilu_scratchfiles_update_center"/>
            </file>
        </folder>
    </folder>
</filesystem>
